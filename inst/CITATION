citHeader("To cite GFLlogit in publications use:")

citEntry(
  entry    = "Manual",
  title    = "GFLlogit: Coordinate optimization for GFL logistic regression",
  author   = "Ohishi M.",
  year     = 2021,
  note     = "R package version 1.0.0",
  url      = "https://github.com/ohishim/GFLlogit",
  textVersion = paste("
    Ohishi, M. (2021).
    GFLlogit: Coordinate optimization for GFL logistic regression.
    R package version 1.0.0.
    https://github.com/ohishim/GFLlogit
  ")
)
